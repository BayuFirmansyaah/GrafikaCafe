                <h2 class="f-nunito-5">Data Laporan</h2>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-3">
                <div class="card-total">
                    <h5>Total Pendapatan Bulan Ini</h5>
                    <h3 id="pendapatan-hari-ini">Rp. 0,-</h3>
                </div>
            </div>
        </div>

        <div class="row bar-filter">
            <div class="col-lg-3">
                <select class="form-control" id="kasir-filter" style="height: 80%;margin-top:5px">
                    
                </select>
            </div>
            <div class="col-lg-1">
                <button class="btn btn-primary" id="filter" style="height: 80%;margin-top:5px">Filter</button>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12">
                <table class="table table-bordered">
                    <thead>
                      <tr>
                        <th scope="col">No</th>
                        <th scope="col">Petugas</th>
                        <th scope="col">Tanggal</th>
                        <th scope="col">Pendapatan</th>
                      </tr>
                    </thead>
                    <tbody>
                     
                    </tbody>
                  </table>
            </div>
        </div>
    </div>
